{
  "name": "agefans",
  "version": "1.23.8",
  "description": "增强agefans播放功能，实现自动换集、无缝换集、画中画、历史记录、断点续播、弹幕等功能",
  "main": "index.user.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IronKinoko/agefans-enhance.git"
  },
  "keywords": [
    "agefans",
    "userscirpt"
  ],
  "author": "IronKinoko <kinoko_main@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IronKinoko/agefans-enhance/issues"
  },
  "homepage": "https://github.com/IronKinoko/agefans-enhance#readme",
  "scripts": {
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production",
    "lint": "tsc --noEmit",
    "prepare": "husky install",
    "deploy": "gh-pages -d dist",
    "version": "npm run build && npm run deploy",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jquery": "^3.5.14",
    "@types/lodash-es": "^4.17.6",
    "eslint": "^7.32.0",
    "gh-pages": "^3.2.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-styles": "^3.14.1",
    "sass": "^1.49.11",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@ironkinoko/danmaku": "^1.2.6",
    "hls.js": "^1.1.5",
    "jquery": "^3.6.0",
    "lodash-es": "^4.17.21",
    "plyr": "^3.6.12"
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "lint-staged": {
    "src/**/*.{js,ts,sass}": [
      "prettier -c -w"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
